{
  "presets": [
    ["@babel/preset-env"],
    ["env" {
      "modules": false
    }],
    ["es2015", { "modules": false }],
    ["stage-0"]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime"],
    [
      "component",
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ]
  ]
}